{
    "name": "sonatra/doctrine-extensions-bundle",
    "type": "symfony-bundle",
    "description": "Add doctrine unique entity validator",
    "keywords": ["doctrine", "extensions", "validator", "unique entity"],
    "homepage": "https://github.com/sonatra/sonatra-doctrine-extensions-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/framework-bundle": "^3.4.0|^4.0.0",
        "sonatra/doctrine-extensions": "^1.0.0"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\DoctrineExtensionsBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
